{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Chunkr",
  "colors": {
    "primary": "#8D8D86",
    "light": "#FFFFFF",
    "dark": "#000000"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "docs/get-started/overview",
              "docs/get-started/quickstart",
              "docs/get-started/llm-docs",
              "docs/get-started/playground"
            ]
          },
              {
                "group": "Task System",
                "icon": "upload",
                "pages": [
                  "docs/task-system/overview",
                  "docs/task-system/task-handling",
                  {
                    "group": "Webhooks",
                    "pages": [
                      "docs/task-system/webhooks/overview",
                      "docs/task-system/webhooks/troubleshooting",
                      "docs/task-system/webhooks/examples"
                    ]
                  },
                  "docs/task-system/file-types",
                  "docs/task-system/limits"
                 
                ]
              },
              {
                "group": "Parse",
                "icon": "crop-simple",
                "pages": [
                  "docs/parse/overview",
                  "docs/parse/outputs",
                  "docs/parse/best-practices"
                ]
              }
            
          ,
          {
            "group": "Models",
            "icon": "sparkles",
            "pages": ["docs/models/overview", "docs/models/chunkr-parse-1"]
          },
          {
            "group": "Advanced",
            "icon": "gear",
            "pages": [
              {
                "group": "Task System",
                "pages": ["docs/advanced/upload/full-CRUD"]
              },
              {
                "group": "Parse",
                "pages": ["docs/advanced/parse/defaults","docs/advanced/parse/configuration"]
              },
              "docs/advanced/error-handling"
            ]
          },
          {
            "group": "Security",
            "icon": "shield-check",
            "pages": [
              "docs/security/overview",
              "docs/security/TOS",
              "docs/security/data-retention",
              "docs/security/certifications",
              "docs/security/on-premise"
            ]
          }
        ]
      },
      {
        "tab": "API References",
        "openapi": {
          "source": "https://app.stainless.com/api/spec/documented/chunkr/openapi.documented.yml",
          "directory": "api-references"
        }
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Sign Up",
          "href": "https://www.chunkr.ai",
          "icon": "user-plus"
        },
        {
          "anchor": "Discord",
          "href": "https://discord.gg/XzKWFByKzW",
          "icon": "discord"
        },
        {
          "anchor": "X",
          "href": "https://x.com/chunkrai",
          "icon": "x-twitter"
        },
        {
          "anchor": "LinkedIn",
          "href": "https://www.linkedin.com/company/chunkr",
          "icon": "linkedin"
        }
      ]
    }
  },
  "styling": {
    "codeblocks": "system"
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://www.chunkr.ai"
  },
  "customJS": ["/assets/js/custom.js"],
  "api": {
    "playground": {
      "display": "interactive",
      "proxy": false
    },
    "mdx": {
      "auth": {
        "method": "key"
      }
    }
  },
  "background": {},
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:ishaan@lumina.sh"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Try Chunkr",
      "href": "https://www.chunkr.ai"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/lumina-ai-inc/chunkr",
      "discord": "https://discord.gg/XzKWFByKzW",
      "x": "https://x.com/chunkrai"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "chatgpt",
      "mcp",
      "cursor",
      {
        "title": "Request a feature",
        "description": "Join the discussion on Discord to request a new feature",
        "icon": "plus",
        "href": "https://discord.gg/XzKWFByKzW"
      }
    ]
  }
}
