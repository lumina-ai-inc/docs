{
  "openapi": "3.0.3",
  "info": {
    "title": "server",
    "description": "",
    "license": {
      "name": ""
    },
    "version": "0.1.0"
  },
  "paths": {
    "/api/figure/{figure_id}/url": {
      "get": {
        "tags": [
          "figure"
        ],
        "operationId": "get_figure_url",
        "parameters": [
          {
            "name": "figure_id",
            "in": "path",
            "description": "The ID of the figure",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful retrieval of figure URL",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FigureUrl"
                }
              }
            }
          },
          "404": {
            "description": "Figure not found"
          }
        }
      }
    },
    "/api/file/{file_id}": {
      "get": {
        "tags": [
          "crate::api_routes::metadata"
        ],
        "operationId": "get_file_metadata",
        "parameters": [
          {
            "name": "file_id",
            "in": "path",
            "description": "The ID of the file",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "File metadata retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FileMetadata"
                }
              }
            }
          },
          "404": {
            "description": "File metadata not found"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/api/file/{file_id}/url": {
      "get": {
        "tags": [
          "crate::api_routes::metadata"
        ],
        "operationId": "get_file_url",
        "parameters": [
          {
            "name": "file_id",
            "in": "path",
            "description": "The ID of the file",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "File URL retrieved successfully",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/api/search": {
      "post": {
        "tags": [
          "search"
        ],
        "operationId": "search",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SearchPayload"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful search",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SearchResult"
                }
              }
            }
          },
          "400": {
            "description": "Bad request"
          }
        }
      }
    },
    "/api/search/figures": {
      "post": {
        "tags": [
          "search"
        ],
        "operationId": "figure_search_route",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/FigureSearchPayload"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful figure search",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FigureSearchResult"
                }
              }
            }
          },
          "400": {
            "description": "Bad request"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "FigureUrl": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "FileMetadata": {
        "type": "object",
        "required": [
          "file_id",
          "title",
          "date_published",
          "sources",
          "types"
        ],
        "properties": {
          "abstract": {
            "type": "string",
            "nullable": true
          },
          "authors": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Author"
            },
            "nullable": true
          },
          "citation_count": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "date_published": {
            "type": "string"
          },
          "doi": {
            "type": "string",
            "nullable": true
          },
          "file_id": {
            "type": "string"
          },
          "has_pdf": {
            "type": "boolean",
            "nullable": true
          },
          "is_oa": {
            "type": "boolean",
            "nullable": true
          },
          "journals": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Journal"
            },
            "nullable": true
          },
          "open_alex_id": {
            "type": "string",
            "nullable": true
          },
          "pmid": {
            "type": "string",
            "nullable": true
          },
          "sources": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "title": {
            "type": "string"
          },
          "topics": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Topic"
            },
            "nullable": true
          },
          "types": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "FileUrl": {
        "type": "object",
        "required": [
          "url"
        ],
        "properties": {
          "url": {
            "type": "string"
          }
        }
      },
      "SearchPayload": {
        "type": "object",
        "required": [
          "query",
          "search_type",
          "highlight_results"
        ],
        "properties": {
          "filters": {
            "allOf": [
              {
                "$ref": "#/components/schemas/Filters"
              }
            ],
            "nullable": true
          },
          "highlight_delimiters": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "highlight_max_length": {
            "type": "integer",
            "format": "int32",
            "nullable": true,
            "minimum": 0
          },
          "highlight_max_num": {
            "type": "integer",
            "format": "int32",
            "nullable": true,
            "minimum": 0
          },
          "highlight_results": {
            "type": "boolean"
          },
          "highlight_threshold": {
            "type": "number",
            "format": "float",
            "nullable": true
          },
          "page": {
            "type": "integer",
            "format": "int32",
            "nullable": true,
            "minimum": 0
          },
          "page_size": {
            "type": "integer",
            "format": "int32",
            "nullable": true,
            "minimum": 0
          },
          "query": {
            "type": "string"
          },
          "recency_bias": {
            "type": "number",
            "format": "float",
            "nullable": true
          },
          "score_threshold": {
            "type": "number",
            "format": "float",
            "nullable": true
          },
          "search_type": {
            "type": "string"
          }
        }
      },
      "SearchResult": {
        "type": "object",
        "required": [
          "title"
        ],
        "properties": {
          "abstract_chunk": {
            "allOf": [
              {
                "$ref": "#/components/schemas/AbstractChunk"
              }
            ],
            "nullable": true
          },
          "doi": {
            "type": "string",
            "nullable": true
          },
          "id": {
            "type": "string",
            "nullable": true
          },
          "metadata": {
            "allOf": [
              {
                "$ref": "#/components/schemas/Metadata"
              }
            ],
            "nullable": true
          },
          "title": {
            "type": "string"
          }
        }
      }
    }
  },
  "tags": [
    {
      "name": "search",
      "description": "Search endpoints"
    },
    {
      "name": "figure",
      "description": "Figure endpoints"
    },
    {
      "name": "file",
      "description": "File endpoints"
    }
  ]
}