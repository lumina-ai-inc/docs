{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Chunkr",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "sidebar": {
    "items": "container"
  },
  "theme": "venus",
  "layout": "topnav",
  "codeBlock": {
    "mode": "auto"
  },
  "background": {
    "style": "gradient"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#8D8D86",
    "light": "#FFFFFF",
    "dark": "#000000",
    "anchors": {
      "from": "#8D8D86",
      "to": "#8D8D86"
    }
  },
  "api": {
    "auth": {
      "method": "key"
    },
    "playground": {
      "disableProxy": true
    }
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:ishaan@lumina.sh"
    }
  ],
  "topbarCtaButton": {
    "name": "Try Chunkr",
    "url": "https://www.chunkr.ai"
  },
  "tabs": [
    {
      "name": "Documentation",
      "url": "docs/"
    },
    {
      "name": "SDK",
      "url": "sdk/"
    },
    {
      "name": "API References",
      "url": "api-references",
      "openapi": "https://api.chunkr.ai/docs/openapi.json"
    }
  ],
  "anchors": [
    {
      "name": "GitHub",
      "url": "https://github.com/lumina-ai-inc/chunkr",
      "icon": "github"
    },
    {
      "name": "Discord",
      "url": "https://discord.gg/XzKWFByKzW",
      "icon": "discord"
    },
    {
      "name": "Twitter",
      "url": "https://x.com/chunkrai",
      "icon": "x"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": [
        "docs/get-started/overview",
        "docs/get-started/quickstart",
        "docs/get-started/changelog"
      ]
    },
    {
      "group": "Features",
      "pages": [
        "docs/features/overview",
        {
          "group": "Layout Analysis",
          "pages": [
            "docs/features/layout-analysis/what",
            "docs/features/layout-analysis/segmentation_strategy"
          ]
        },
        "docs/features/segment-processing",
        "docs/features/ocr",
        "docs/features/chunking",
        "docs/features/pipeline"
      ]
    },
    {
      "group": "Use Cases",
      "pages": [
        "docs/use-cases/bulk-upload",
        "docs/use-cases/usage-patterns"
      ]
    },
    {
      "group": "Self Hosting",
      "pages": [
        "docs/self-hosting/docker-compose",
        "docs/self-hosting/kubernetes"
      ]
    },
    {
      "group": "Getting Started",
      "pages": [
        "sdk/installation",
        "sdk/usage"
      ]
    },
    {
      "group": "Data Operations",
      "pages": [
        "sdk/data-operations/create",
        "sdk/data-operations/get",
        "sdk/data-operations/update",
        "sdk/data-operations/delete",
        "sdk/data-operations/cancel"
      ]
    },
    {
      "group": "Utilities",
      "pages": [
        "sdk/configuration",
        "sdk/polling",
        "sdk/export"
      ]
    }
  ],
  "footerSocials": {
    "github": "https://github.com/lumina-ai-inc/chunkr",
    "discord": "https://discord.gg/XzKWFByKzW",
    "x": "https://x.com/chunkrai"
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  }
